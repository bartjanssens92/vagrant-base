[global_config]
  borderless = True
  inactive_color_offset = 1.13
  suppress_multiple_term_dialog = True
  title_inactive_fg_color = "#ffffff"
  window_state = maximise
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#aaaaaa"
    scrollback_lines = 10000
    split_to_group = True

